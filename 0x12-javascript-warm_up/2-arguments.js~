#!/usr/bin/node
// Using process.argv to get command line arguments
const args = process.argv.slice(2);

// Checking the number of arguments and printing the corresponding message
if (args.length === 0) {
    console.log("No argument");
} else if (args.length === 1) {
    console.log("Argument found");
} else {
    console.log("Arguments found");
}
